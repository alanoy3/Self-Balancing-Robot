			function getFont(treeId, node) {
			  return node.font ? node.font : {};
			}
			
			function searchNode(nodes, key){
			  if(jq.trim(key).length == 0) {
				return nodes;
			  } else {
				return jQuery.grep(nodes, function(node) {
				  return node["name"].match(eval('/'+key+'/i'));
				})
			  }
			}
			

			jq(document).ready(function(){			
			  jq.fn.zTree.init(jq("#menu"), setting, zNodes);
			  
			  jq('#search_input').keyup(function(){
				jq.fn.zTree.init(jq("#menu"), setting, searchNode(zNodes, jq(this).val()));
			  })
			  
			  if(getcookie('eew_left_mn_enable')==1){
			      //do_hidden();
			  }else{
			      //do_show();
			  }
			  
			  jq('.toggle_side').click(function(){
				if(jq(this).hasClass('hidden_side') ) {
                  do_hidden();
				  setcookie('eew_left_mn_enable',1,2592000);
				  //jq('.side').hide();
				} else {
				  //jq('.side').css('left', "0px");
                  do_show();
				  setcookie('eew_left_mn_enable',2,2592000);
				}
			  });
			  
			  function do_hidden(){
				  jq('#eewleftmn').css('width',0);
				  jq('#eewcontent').css({'width':'100%','padding-left':'0px'});
				  jq('.toggle_side').removeClass('hidden_side');
				  jq('.toggle_side').addClass('show_side');
				  jq.get('sideclose.api.php',{isclose:1});
			  }

			  function do_show(){
				  jq('#eewleftmn').css('width','215px');
				  jq('#eewcontent').css({'width':'215px','padding-left':'217px'});				  
				  jq('.toggle_side').addClass('hidden_side');
				  jq('.toggle_side').removeClass('show_side');
                  jq.get('sideclose.api.php',{isclose:0});				  
			  }
			  
			});
			